<%- pages.map(page => `import React, { useEffect, useState } from 'react';\n\nexport function ${page.name}Details({ id }: { id: string }) {\n  const [data, setData] = useState(null);\n  useEffect(() => {\n    fetch(\`/api/${page.entity.toLowerCase()}/\${id}\`)\n      .then(res => res.json())\n      .then(setData);\n  }, [id]);\n  return <div>${page.entity} Details: {JSON.stringify(data)}</div>;\n}`) .join('\n\n') %>
